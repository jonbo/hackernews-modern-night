{
  "manifest_version": 2,
  "name": "HNews Mobile Dark Blue",
  "version": "1.3.0",

  "description": "Adds styling to HNews Mobile.",

  "content_scripts": [
    {
      "matches": ["*://news.ycombinator.com/*"],
      "css": ["hnews.css"],
      "js": ["hnews.js"],
      "run_at": "document_start"
    }
  ]
}
